<?xml version="1.0" encoding="utf-8"?>
<interface>
    <template class="TaskBox" parent="GtkBox">
        <property name="orientation">vertical</property>
        <property name="margin-top">5</property>
        <property name="margin-bottom">5</property>
        <property name="spacing">3</property>
        <child>
            <object class="GtkBox">
                <child>
                    <object class="GtkLabel" id="file_name">
                        <property name="label">file_name</property>
                        <property name="xalign">0</property>
                        <attributes>
                            <attribute name="weight" value="bold" />
                        </attributes>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel" id="file_size">
                        <property name="label">35MB</property>
                        <property name="xalign">1</property>
                        <property name="hexpand">1</property>
                        <attributes>
                            <attribute name="weight" value="light" />
                        </attributes>
                    </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkProgressBar" id="progress"></object>
        </child>
    </template>
</interface>